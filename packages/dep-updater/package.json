{
  "name": "dep-updater",
  "version": "0.0.1",
  "description": "Automated dependency update tool with Expo support, stacked PRs, and AI-powered changelogs",
  "type": "module",
  "bin": {
    "dep-updater": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@clack/prompts": "^0.7.0",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "yaml": "^2.8.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "memfs": "^4.17.0"
  },
  "keywords": [
    "dependencies",
    "updates",
    "expo",
    "automation",
    "changelog",
    "ai",
    "stacked-prs"
  ],
  "private": true
}
